{
	"name": "mapsomethings",
	"version": "1.0.0",
	"description": "An app to map some things",
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"server": "nodemon server.js",
		"client-install": "npm install --prefix client",
		"client-build": "npm run build --prefix client",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"heroku-postbuild": "npm run client-install && npm run client-build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/calebbsides/mapsomethings.git"
	},
	"author": "Caleb Sides",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/calebbsides/mapsomethings/issues"
	},
	"homepage": "https://github.com/calebbsides/mapsomethings#readme",
	"dependencies": {
		"body-parser": "^1.18.3",
		"bootstrap": "^4.1.3",
		"concurrently": "^4.1.0",
		"express": "^4.16.4",
		"mongoose": "^5.3.15",
		"node-sass": "^4.10.0"
	},
	"devDependencies": {
		"nodemon": "^1.18.7"
	}
}
